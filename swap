#include <iostream>

using namespace std;
void swap(int &a, int &b);
int main()
{
    int a,b;
    cout<<"enter value of a and b";
    cin>>a>>b;
    cout<<"before swapping = \n " <<a<<endl<<b<<endl;
    swap(a,b);
    cout<<"after swapping = "<<a<<endl<<b;
    return 0;
    
}

void swap(int &x, int &y)
{
    int temp;
    temp = x;
    x = y;
    y = temp;
}

